{
  "name": "muinjs/pcl",
  "type": "cakephp-plugin",
  "description": "CakePHP 3 Permission Control Plugin",
  "keywords": [
    "CakePHP",
    "Permission",
    "Acl"
  ],
  "authors": [
    {
      "name": "A K M Mainul Islam",
      "email": "muinjs@gmail.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "~3.0",
    "cakephp/acl": "^0.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Pcl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pcl\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
